      <TextField fx:id="piece" layoutX="205.0" layoutY="115.0" onAction="#factvalid" style="-fx-background-radius: 100; -fx-border-color: #9efd38;" />
      <TextField fx:id="article" layoutX="205.0" layoutY="213.0" onAction="#refartvalid" style="-fx-background-radius: 100; -fx-border-color: #9efd38;" />
      <Label fx:id="error" layoutX="90.0" layoutY="355.0" textFill="RED">
         <font>
            <Font size="13.0" />
         </font>
      </Label>
      <Label layoutX="104.0" layoutY="30.0" text="Ajouter Article">
         <font>
            <Font name="System Bold" size="36.0" />
         </font>
      </Label>
