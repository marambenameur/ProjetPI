<AnchorPane id="AnchorPane" prefHeight="508.0" prefWidth="413.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.AjoutartfactController">
   <children>
      <TextField fx:id="qt" layoutX="205.0" layoutY="315.0" onAction="#qtvalid" prefHeight="25.0" prefWidth="149.0" style="-fx-background-radius: 100; -fx-border-color: #9efd38;" />
      <Label layoutX="79.0" layoutY="266.0" text="Libelle :">
         <font>
            <Font size="15.0" />
         </font>
      </Label>
      <Label layoutX="80.0" layoutY="319.0" text="Quantité :">
         <font>
            <Font size="15.0" />
         </font>
      </Label>
      <Button fx:id="valide" layoutX="216.0" layoutY="393.0" mnemonicParsing="false" onAction="#valider" prefHeight="29.0" prefWidth="81.0" style="-fx-background-color: #9efd38; -fx-background-radius: 100;" text="Valider" textFill="WHITE">
         <font>
            <Font name="System Bold" size="13.0" />
         </font>
      </Button>
      <Button fx:id="annule" layoutX="316.0" layoutY="393.0" mnemonicParsing="false" onAction="#annuler" prefHeight="29.0" prefWidth="81.0" style="-fx-background-color: #9efd38; -fx-background-radius: 100;" text="Annuler" textFill="WHITE">
         <font>
            <Font name="System Bold" size="13.0" />
         </font>
      </Button>
      <ComboBox fx:id="lib" layoutX="205.0" layoutY="264.0" onAction="#libvalid" prefWidth="150.0" style="-fx-background-radius: 100; -fx-border-color: #9efd38;" />
