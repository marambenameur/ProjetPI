         <font>
            <Font size="15.0" />
         </font>
      </Label>
      <Label layoutX="79.0" layoutY="215.0" text="Référence Article :">
         <font>
            <Font size="15.0" />
         </font>
      </Label>
      <Label layoutX="80.0" layoutY="168.0" text="Type :">
         <font>
            <Font size="15.0" />
         </font>
      </Label>
      <ComboBox fx:id="type" layoutX="202.0" layoutY="164.0" onAction="#typevalid" prefWidth="150.0" style="-fx-background-radius: 100; -fx-border-color: #9efd38;" />
